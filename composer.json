{
	"name": "orisai/nette-monolog",
	"description": "Monolog logger integration for Nette",
	"keywords": [
		"logger",
		"logging",
		"logtail",
		"monolog",
		"nette",
		"orisai",
		"psr-3",
		"tracy"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/nette-monolog",
	"authors": [
		{
			"name": "Marek BartoÅ¡",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": ">=7.4.0 <8.2.0",
		"monolog/monolog": "^2.3.0",
		"nette/di": "^3.0.6",
		"nette/schema": "^1.1.0",
		"orisai/exceptions": "^1.1.0",
		"orisai/nette-di": "^1.0.3",
		"psr/log": "^1.1.0|^2.0.0|^3.0.0"
	},
	"require-dev": {
		"brianium/paratest": "^6.3.0",
		"infection/infection": "^0.25.0",
		"nette/application": "^3.0.0",
		"orisai/coding-standard": "^2.0.0",
		"orisai/nette-http-client": "^1.0.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^1.0.0",
		"phpstan/phpstan-deprecation-rules": "^1.0.0",
		"phpstan/phpstan-nette": "^1.0.0",
		"phpstan/phpstan-phpunit": "^1.0.0",
		"phpstan/phpstan-strict-rules": "^1.0.0",
		"phpunit/phpunit": "^9.5.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.7.0",
		"symfony/http-client": "^5.3.0|^6.0.0",
		"tracy/tracy": "^2.8.0"
	},
	"autoload": {
		"psr-4": {
			"OriNette\\Monolog\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\OriNette\\Monolog\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	}
}

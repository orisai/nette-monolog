includes:
	- phpstan.base.neon

parameters:
	level: max
	resultCachePath: %currentWorkingDirectory%/var/tools/PHPStan/resultCache.src.php

	ignoreErrors:
		- message: '#^Parameter \#1 \$record of (.*)HandlerInterface\:\:handle\(\) expects array\((.*)\), array given\.$#'
		  path: ../src/HandlerAdapter.php
		  count: 1
